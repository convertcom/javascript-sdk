{
  "name": "@convertcom/js-sdk-demo-react",
  "version": "1.0.0",
  "author": "Convert Insights, Inc",
  "license": "Apache-2.0",
  "dependencies": {
    "feather-icons": "^4.29.2",
    "framer-motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-modal": "^3.16.3",
    "react-router-dom": "^7.9.5",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19",
    "tailwindcss": "^3.4.4",
    "twin.macro": "^3.4.1"
  },
  "peerDependencies": {
    "@convertcom/js-sdk": ">=2.0.0"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build"
  },
  "watch": {
    "build": {
      "patterns": [
        "."
      ],
      "ignore": "build",
      "extensions": "*",
      "quiet": false
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "src/**/*.js"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off",
          "react-hooks/exhaustive-deps": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
